{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RepositoryOptions": {
    "CosmosConnectionString": "<CosmosDB-ConnectionString>",
    "DatabaseId": "<CosmosDB-DatabaseName>",
    "ContainerId": "<CosmosDB-ContainerName>",
    "SerializationOptions": {
      "PropertyNamingPolicy": "CamelCase"
    }
  }
}
